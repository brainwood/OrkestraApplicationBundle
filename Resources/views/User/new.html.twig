{% extends '::base.html.twig' %}

{% block title %}New User{% endblock %}

{% block content %}
<form action="{{ path('orkestra_user_create') }}" method="post" {{ form_enctype(form) }}>
<div class="well">
    {{ form_widget(form) }}
    <div class="control-group">
        <div class="controls">
            <button type="submit" class="btn btn-primary">Create User</button>
        </div>
    </div>
</div>
</form>
{% endblock %}
